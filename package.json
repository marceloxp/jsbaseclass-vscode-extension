{
    "name": "jsbaseclass-vscode-extension",
    "displayName": "JsBaseClass Extension",
    "description": "A VS Code extension to quickly generate JavaScript classes extending JsBaseClass. Simplifies creating modular and reusable components with a customizable template.",
    "version": "0.0.1",
    "publisher": "marceloxp",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "keywords": [
        "javascript",
        "jsbaseclass",
        "template",
        "class generator",
        "productivity"
    ],
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/marceloxp/jsbaseclass-vscode-extension"
    },
    "bugs": {
        "url": "https://github.com/marceloxp/jsbaseclass-vscode-extension/issues"
    },
    "license": "MIT",
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.createJsBaseClass",
                "title": "New JsBaseClass",
                "category": "JsBaseClass"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.96.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "eslint": "^9.16.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1"
    }
}